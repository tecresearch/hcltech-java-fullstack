<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">
        
 <bean  class="in.tca.beans.Address" id="addId">
 	<property name="area" value="Mokalpur Gobrahan"/>
 	<property name="district" value="Varanasi"/>
 	<property name="state" value="Uttar Pradesh"/>
 	<property name="pin" value="221104"/>
 </bean>

<bean  class="in.tca.beans.Student" id="stdId">
		<property name="id" value="101"/>
		<property name="name" value="Brijesh Nishad"/>
		<property name="phone" value="9506563150"/>
		<property name="address" ref="addId"/>
</bean>
</beans>